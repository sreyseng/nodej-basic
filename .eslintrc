{
    "extends": "airbnb",
    "parserOptions": {
        "ecmaVersion": 6
    },
    "env": {
        "node": true,
        "browser": true,
        "es6": true,
        "mocha": true
    },
    "rules": {
        "comma-dangle": ["error", "never"],
        "no-console": [
          "error", {
            "allow": ["info", "warn", "error"]
          }
        ]
    }
}